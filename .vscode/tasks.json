{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: gcc 生成活动文件",
            "command": "/usr/bin/gcc",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "${workspaceFolder}/src/cJSON.c",
                "${workspaceFolder}/src/DataStruct.c",
                "${workspaceFolder}/src/JsonProcess.c",
                "${workspaceFolder}/src/UDPConnector.c",
                "${workspaceFolder}/src/main.c",
                "-l",
                "pthread",
                "-o",
                "${workspaceFolder}/bin/registry_task"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "调试器生成的任务。"
        }
    ],
    "version": "2.0.0"
}